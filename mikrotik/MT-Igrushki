/snmp
set contact=helptech@smartics.ru enabled=yes engine-id=744D28B5CC8B location=Igrushki trap-generators=interfaces trap-interfaces=all trap-target=192.168.0.62,10.10.3.34 trap-version=3
/system clock
set time-zone-name=Europe/Moscow
/system identity
set name=Igrushki
/system leds
add leds="wlan2_signal1-led,wlan2_signal2-led,wlan2_signal3-led,wlan2_signal4-led,wlan2_signal5-led" type=wireless-signal-strength
add leds=wlan2_tx-led type=interface-transmit
add leds=wlan2_rx-led type=interface-receive
/system logging
add action=zabbix topics=system
add action=zabbix topics=script
/system scheduler
add interval=20m name=VPN-disconnect on-event="/system script run Snytkin-vpn-info;" policy=ftp,reboot,read,write,policy,test,password,sniff,sensitive,romon start-date=mar/30/2023 start-time=12:30:00
/system script
add dont-require-permissions=no name=script1 owner=admin policy=ftp,reboot,read,write,policy,test,password,sniff,sensitive,romon source="/interface wireless cap\r\n# \r\nset caps-man-addresses=192.168.0.1 enabled=yes interfaces=wlan_2.4,wlan_5Ghz"
add dont-require-permissions=no name=L2tpDisconnected owner=eduard policy=ftp,reboot,read,write,policy,test,password,sniff,sensitive,romon source=":local DeviceName [/system identity get name];\r\n:local Time [/system clock get time];\r\n:local Date [/system clock get date];\r\n:local TelegramMessageText;\r\n:local TGtoken bot326215219:AAEBBG6NGikAj7MHNFnAXvqfVqT6Shc8inE/;\r\n:local ChatIDTG -1001480396546;\r\n\r\n:global L2tpDisconnect;\r\n\r\n:local IDsEvents [/log find where topics~\"l2tp\" message~\".l2tp.*disconnected\"];\r\n:local LenArrayIDs [:len \$IDsEvents];\r\n:local StartArrayID [:find \$IDsEvents \$L2tpDisconnect];\r\n:local EndArrayID (\$IDsEvents -> (\$LenArrayIDs-1));\r\n\r\n:log info \"Script ParseLogLoginFailure: running.\";\r\n\r\n:if (\$EndArrayID != \$L2tpDisconnect and [:tobool \$L2tpDisconnect] ) do={\r\n\r\n    :log info \"Script ParseLogLoginFailure: new events found.\";\r\n\r\n    :for KeyArray from=(\$StartArrayID+1) to=(\$LenArrayIDs-1) do={\r\n        :local IDMessage (\$IDsEvents ->\$KeyArray );\r\n        :set TelegramMessageText \"\$TelegramMessageText %0D%0A  \$[/log\_get number=\$IDMessage time] - \$[/log get number=\$IDMessage message];\";\r\n        }\r\n\r\n    :set L2tpDisconnect \$EndArrayID;\r\n\r\n    :log info \"Script ParseLogLoginFailure: events processed. Sending notifications.\";\r\n\r\n    # START SEND TELEGRAM\r\n    :local MessageText \"\\E2\\9B\\94 \$DeviceName: L2TP Shutdown \$TelegramMessageText\";\r\n    /tool fetch url=\"https://api.telegram.org/\$TGtoken/sendMessage\\\?chat_id=\$ChatIDTG&text=\$MessageText\" keep-result=no;\r\n    # END SEND TELEGRAM\r\n\r\n} else={\r\n  :log info \"Script ParseLogLoginFailure: no new messages found.\";\r\n};\r\n\r\n#:log info \"Script ParseLogLoginFailure: script completed successfully.\";\r\n:set L2tpDisconnect \$EndArrayID;"
add dont-require-permissions=no name=OvpnDisconnected owner=eduard policy=ftp,reboot,read,write,policy,test,password,sniff,sensitive,romon source=":local DeviceName [/system identity get name];\r\n:local Time [/system clock get time];\r\n:local Date [/system clock get date];\r\n:local TelegramMessageText;\r\n:local TGtoken bot326215219:AAEBBG6NGikAj7MHNFnAXvqfVqT6Shc8inE/;\r\n:local ChatIDTG -1001480396546;\r\n\r\n:global OvpnDisconnect;\r\n\r\n:local IDsEvents [/log find where topics~\"ovpn\" message~\".ovpn.*disconnected\"];\r\n:local LenArrayIDs [:len \$IDsEvents];\r\n:local StartArrayID [:find \$IDsEvents \$OvpnDisconnect];\r\n:local EndArrayID (\$IDsEvents -> (\$LenArrayIDs-1));\r\n\r\n:log info \"Script ParseLogLoginFailure: running.\";\r\n\r\n:if (\$EndArrayID != \$OvpnDisconnect and [:tobool \$OvpnDisconnect] ) do={\r\n\r\n    :log info \"Script ParseLogLoginFailure: new events found.\";\r\n\r\n    :for KeyArray from=(\$StartArrayID+1) to=(\$LenArrayIDs-1) do={\r\n        :local IDMessage (\$IDsEvents ->\$KeyArray );Snytkin\r\n        :set TelegramMessageText \"\$TelegramMessageText %0D%0A  \$[/log\_get number=\$IDMessage time] - \$[/log get number=\$IDMessage message];\";\r\n        }\r\n\r\n    :set OvpnDisconnect \$EndArrayID;\r\n\r\n    :log info \"Script ParseLogLoginFailure: events processed. Sending notifications.\";\r\n\r\n    # START SEND TELEGRAM\r\n    :local MessageText \"\\E2\\9B\\94 \$DeviceName: OVPN Shutdown \$TelegramMessageText\";\r\n    /tool fetch url=\"https://api.telegram.org/\$TGtoken/sendMessage\\\?chat_id=\$ChatIDTG&text=\$MessageText\" keep-result=no;\r\n    # END SEND TELEGRAM\r\n\r\n} else={\r\n  :log info \"Script ParseLogLoginFailure: no new messages found.\";\r\n};\r\n\r\n#:log info \"Script ParseLogLoginFailure: script completed successfully.\";\r\n:set OvpnDisconnect \$EndArrayID;"
add dont-require-permissions=no name=Snytkin-vpn-info owner=eduard policy=ftp,reboot,read,write,policy,test,password,sniff,sensitive,romon source=":local DeviceName [/system identity get name];\r\n:local Time [/system clock get time];\r\n:local Date [/system clock get date];\r\n\r\n:local TelegramMessageText;\r\n:local TGtoken bot326215219:AAEBBG6NGikAj7MHNFnAXvqfVqT6Shc8inE/;\r\n:local ChatIDTG -1001480396546;\r\n\r\n:local User Snytkin;\r\n\r\n:global LogLableVPN1;\r\n\r\n:local IDsEvents [/log find where topics~\"error\" message~\".*\$User.*authentication failed\"];\r\n\r\n:local LenArrayIDs [:len \$IDsEvents];\r\n:local StartArrayID [:find \$IDsEvents \$LogLableVPN1];\r\n:local EndArrayID (\$IDsEvents -> (\$LenArrayIDs-1));\r\n\r\n:log info \"Script ParseLogLoginFailure: running.\";  \r\n\r\n:if (\$EndArrayID != \$LogLableVPN1 and [:tobool \$LogLableVPN1] ) do={\r\n\r\n    :log info \"Script ParseLogLoginFailure: new events found.\";\r\n\r\n    :for KeyArray from=(\$StartArrayID+1) to=(\$LenArrayIDs-1) do={\r\n        :local IDMessage (\$IDsEvents ->\$KeyArray );\r\n        :set TelegramMessageText \"\$TelegramMessageText %0D%0A  \$[/log\_get number=\$IDMessage time] - \$[/log get number=\$IDMessage message];\";\r\n        }\r\n\r\n    :set LogLableVPN1 \$EndArrayID;\r\n\r\n    :log info \"Script ParseLogLoginFailure: events processed. Sending notifications.\";\r\n\r\n    # START SEND TELEGRAM\r\n    :local MessageText \"\\E2\\9B\\94 \$DeviceName: VPN ERROR by \$User \$TelegramMessageText\";\r\n    /tool fetch url=\"https://api.telegram.org/\$TGtoken/sendMessage\\\?chat_id=\$ChatIDTG&text=\$MessageText\" keep-result=no;\r\n    # END SEND TELEGRAM\r\n\r\n} else={\r\n  :log info \"Script ParseLogLoginFailure: no new messages found.\";\r\n};\r\n\r\n#:log info \"Script ParseLogLoginFailure: script completed successfully.\";\r\n:set LogLableVPN1 \$EndArrayID;"
add dont-require-permissions=no name="Ping 77.88.8.8" owner=eduard policy=ftp,reboot,read,write,policy,test,password,sniff,sensitive,romon source="{\r\n:local pinglimit 4;\r\n:local ttllimit 160;\r\n:local sent [/ping address=77.88.8.8 count=\$pinglimit ttl=\$ttllimit]\r\n:local pingloss (100 - 100 / \$pinglimit*\$sent)\r\n:log info message=\"Pings send to 77.88.8.8:\$sent lost:\$pingloss %\"\r\n}"
add dont-require-permissions=no name=tst owner=eduard policy=ftp,reboot,read,write,policy,test,password,sniff,sensitive,romon source="local a \"ping 192.168.1.25 count 4\";\r\nexecute script=\$a file=1.txt"
/tool netwatch
add host=77.88.8.8 interval=2s timeout=5s
add host=8.8.8.8 interval=2s timeout=5s
/tool sniffer
set file-limit=8000KiB file-name=rar.pcap filter-interface=all memory-limit=1500KiB only-headers=yes
/tool traffic-generator packet-template
add mac-protocol=arp name=tst-arp
/tool traffic-generator stream
add name=tst-arp-stream packet-size=200 tx-template=tst-arp
